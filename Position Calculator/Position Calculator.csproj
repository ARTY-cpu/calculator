<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net5.0-windows</TargetFramework>
        <RootNamespace>Position_Calculator</RootNamespace>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="logo.ico" />
      <Resource Include="logo.ico" />
      <None Remove="BWtheme.png" />
      <Resource Include="BWtheme.png">
        <DependentUpon>App.xaml</DependentUpon>
      </Resource>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Binance.Net" Version="8.0.0-alpha2" />
    </ItemGroup>

</Project>
